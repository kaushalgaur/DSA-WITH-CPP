#include<iostream>
using namespace std;
int main (){
 // intializing Array . 
    int a[10]={0};
// Printing the Array Elements.
    for(int i=0;i<10;i++){
        cout<<a[i]<<endl;
    }
    cout<<"Changing the value of array element ."<<endl;
    for(int i=0;i<10;i++){
        a[i]=i+1;
    }
    cout<<"Values changed ."<<endl;
    for(int i=0;i<10;i++){
        cout<<a[i]<<endl;
    }
}
